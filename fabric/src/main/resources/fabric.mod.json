{
  "schemaVersion": 1,
  "id": "brigadierextras",
  "version": "${version}",

  "name": "Brigadier Extras",
  "description": "Implement extra features into Brigadier, now as a Fabric mod!",
  "authors": ["Nova"],
  "contact": {},

  "license": "GPL-3.0",
  "icon": "assets/brigadierextras/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": ["net.nova.brigadierextras.fabric.FabricBrigadierExtras"]
  },

  "mixins": ["brigadierextras.mixins.json"],

  "accessWidener": "brigadierextras.accessWidener",

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/hadcHUfaSk"
      },
      "badges": [ "library" ],
      "update_checker": false
    }
  }
}
